<?xml version="1.0" encoding="utf-8"?>
<executable>
  <category>Examples</category>
  <title>LogisticContrastEnhancement</title>
  <description><![CDATA[This is a CLI module that can be bundled in an extension]]></description>
  <version>0.0.1</version>
  <documentation-url>http://www.example.com/Slicer/Modules/LogisticContrastEnhancement</documentation-url>
  <license>Slicer</license>
  <contributor>FirstName LastName (Institution), FirstName LastName (Institution)</contributor>
  <acknowledgements>This work was partially funded by NIH grant NXNNXXNNNNNN-NNXN</acknowledgements>
  <parameters>
    <label>IO</label>
    <description><![CDATA[Input/output parameters]]></description>
    <image>
      <name>inputVolume</name>
      <label>Input Volume</label>
      <channel>input</channel>
      <index>0</index>
      <description><![CDATA[Input volume]]></description>
    </image>
    <image type="label">
      <name>maskVolume</name>
      <label>Mask Volume</label>
      <channel>input</channel>
      <index>1</index>
      <description><![CDATA[Mask volume]]></description>
    </image>
    <image>
      <name>outputVolume</name>
      <label>Output Volume</label>
      <channel>output</channel>
      <index>2</index>
      <description><![CDATA[Output Volume]]></description>
    </image>
  </parameters>
<parameters advanced="true">
    <label>Advanced Parameters</label>
    <description><![CDATA[Advanced parameters]]></description>
    <integer>
      <name>numberOfBins</name>
      <longflag>--nBins</longflag>
      <flag>n</flag>
      <label>Number Of Bins</label>
      <description><![CDATA[Number of bins to build the histograms]]></description>
      <default>128</default>
	<constraints>
        <minimum>10</minimum>
        <maximum>512</maximum>
        <step>1</step>
      </constraints>
    </integer>
    <boolean>
	      <name>flipObject</name>
	      <longflag>--flipObject</longflag>
	      <flag>f</flag>
	      <description><![CDATA[Invert the contrast enhancement if the object of interest is located at the lower intensity level in the image.]]></description>
	      <label>Flip Object</label>
	      <default>false</default>
	</boolean>
	<string-enumeration>
	      <name>thrType</name>
	      <longflag>--thr</longflag>
              <flag>t</flag>
	      <description><![CDATA[Threshold method use to split object/background from the image histogram.]]></description>
	      <label>Threshold Method</label>
	      <default>MaximumEntropy</default>
	      <element>MaximumEntropy</element>
	      <element>Otsu</element>
	      <element>Renyi</element>
	      <element>Moments</element>
	      <element>Yen</element>
	      <element>IsoData</element>
	</string-enumeration>
</parameters>
</executable>
